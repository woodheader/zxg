.housekeep_tabs {
  width: 100%;
  padding: 60rpx;
  text-align: center;
  background-color: #01b9e5;
  color: #fff;
  font-size: 48rpx;
  font-weight: bold;
  white-space: 20rpx;
}
.housekeep .tabs_content .standards {
  box-shadow: 0 1rpx 5rpx 0px rgba(0, 0, 0, 0.05);
  padding: 20rpx;
  background-color: #fff;
  width: 93vw;
  border-radius: 20rpx;
  margin: 20rpx auto;
  position: relative;
  height: calc(100vh - 320rpx);
}
.housekeep .tabs_content .standards ._h4 {
  font-size: 30rpx;
}
.housekeep .tabs_content .standards ._p {
  line-height: 36rpx;
  margin-top: 30rpx;
  font-size: 30rpx;
}
.housekeep .tabs_content .standards .bth {
  width: 100%;
  display: flex;
  justify-content: center;
}
.housekeep .tabs_content .standards ._h5 {
  display: inline-block;
  border: 2rpx solid rgba(33, 140, 254, 0.5);
  padding: 20rpx 70rpx;
  font-size: 30rpx;
  border-radius: 12rpx;
  margin-top: 40rpx;
}
.housekeep .tabs_content .score_to {
  padding: 20rpx;
}
.housekeep .tabs_content .score_to > view {
  padding: 20rpx;
  box-shadow: 0 1rpx 5rpx 0px rgba(0, 0, 0, 0.05);
  padding: 20rpx;
  background-color: #fff;
  margin-bottom: 20rpx;
  border-radius: 20rpx;
  font-size: 36rpx;
}
.housekeep .tabs_content .score_to > view.score ._i {
  margin-left: 20rpx;
}
.housekeep .tabs_content .score_to .submit {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 30rpx;
}
.housekeep .tabs_content .score_to .submit ._h5 {
  display: inline-block;
  border: 2rpx solid rgba(33, 140, 254, 0.5);
  padding: 20rpx 70rpx;
  border-radius: 12rpx;
}
.housekeep .tabs_content .score_to .submit .submitScr {
  border: 1rpx solid rgba(0, 0, 0, 0.05);
  box-shadow: 1rpx 1rpx 5rpx 1px rgba(0, 0, 0, 0.05);
  background-color: #fff;
  padding: 20rpx;
  flex: 1;
  font-size: 30rpx;
}
.housekeep .tabs_content .score_to .current ._p {
  line-height: 60rpx;
  font-size: 30rpx;
}
.housekeep .tabs_content .score_to .listBox {
  display: flex;
  width: 100%;
}
.housekeep .tabs_content .score_to .listBox .historyStore {
  margin-top: 28rpx;
  flex-shrink: 0;
  border: 1rpx solid rgba(0, 0, 0, 0.05);
  box-shadow: 1rpx 1rpx 5rpx 1px rgba(0, 0, 0, 0.05);
  font-size: 32rpx;
  padding: 20rpx;
  margin-right: 24rpx;
}
.housekeep .tabs_content .score_to .listBox .historyStore ._p {
  font-size: 30rpx;
}
.housekeep .tabs_content .score_to .score ._p {
  display: flex;
  align-items: center;
  line-height: 80rpx;
  font-size: 30rpx;
}
.housekeep .dialog_text {
  padding: 80rpx;
  width: 90vw;
  box-sizing: border-box;
  text-align: center;
  font-size: 36rpx;
}
.housekeep .dialog_footer {
  width: 100%;
  display: flex;
  justify-content: center;
  padding-bottom: 20rpx;
}
.housekeep .dialog_footer .dialog_bth {
  border: 2rpx solid rgba(33, 140, 254, 0.5);
  padding: 20rpx 60rpx;
  text-align: center;
  display: inline-block;
  border-radius: 12rpx;
  background-color: #218cfe;
  color: #fff;
}

